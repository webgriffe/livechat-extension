<?xml version="1.0"?><config>    <tabs>        <livechat translate="label" module="livechat">            <label>LiveChat</label>            <sort_order>210</sort_order>        </livechat>    </tabs>    <sections>        <livechat translate="label" module="livechat">            <label>LiveChat</label>            <tab>livechat</tab>            <frontend_type>text</frontend_type>            <sort_order>130</sort_order>            <show_in_default>1</show_in_default>            <show_in_website>1</show_in_website>            <show_in_store>1</show_in_store>            <groups>                <general translate="label">                    <label>General Settings</label>                    <frontend_type>text</frontend_type>                    <sort_order>10</sort_order>                    <show_in_default>1</show_in_default>                    <show_in_website>1</show_in_website>                    <show_in_store>1</show_in_store>                    <fields>						<license translate="label">							<label>License Number</label>                            <frontend_type>text</frontend_type>                            <sort_order>20</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>1</show_in_store>                        </license>                        <login translate="label">                            <label>Livechat account login</label>                            <frontend_type>text</frontend_type>                            <sort_order>30</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_default>1</show_in_default>                            <show_in_default>1</show_in_default>                        </login>                        <mobile translate="label">                            <label>Hide chat window on mobile</label>                            <frontend_type>text</frontend_type>                            <source_model>livechat/system_config_source_yesno</source_model>                            <sort_order>40</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_default>1</show_in_default>                            <show_in_default>1</show_in_default>                        </mobile>                        <cart translate="label">                            <label>Track your customers cart</label>                            <frontend_type>text</frontend_type>                            <source_model>livechat/system_config_source_yesno</source_model>                            <sort_order>50</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_default>1</show_in_default>                            <show_in_default>1</show_in_default>                        </cart>                    </fields>                </general>				<advanced translate="label">                    <label>Advanced</label>                    <frontend_type>text</frontend_type>                    <sort_order>20</sort_order>                    <show_in_default>1</show_in_default>                    <show_in_website>1</show_in_website>                    <show_in_store>1</show_in_store>                    <fields>						<language translate="label">                            <label>Language</label>                            <frontend_type>select</frontend_type>                            <sort_order>10</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>1</show_in_store>                        </language>                    </fields>                </advanced>            </groups>        </livechat>    </sections></config>